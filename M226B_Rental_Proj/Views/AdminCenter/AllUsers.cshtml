
@{
    ViewBag.Title = "AllUsers";
}

@model AllUsersModell

<h2>AllUsers</h2>

<table class="table table-dark">
    <thead>
        <tr>
            <th scope="col">ID</th>
            <th scope="col">Username</th>
            <th scope="col">Admin</th>
        </tr>
    </thead>
    <tbody>
        @foreach (ApplicationUser user in Model.Users)
        {
            <tr>
                <th scope="row">@user.Id</th>
                <td>@user.UserName</td>
                <td>@user.IsAdmin</td>
            </tr>
        }
    </tbody>
</table>

